{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "crons": [
    { "path": "/api/push/dispatch-daily", "schedule": "0 9 * * *" },
    { "path": "/api/push/dispatch-daily", "schedule": "0 13 * * *" },
    { "path": "/api/push/dispatch-daily", "schedule": "0 18 * * *" }
  ]
}

